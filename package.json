{
  "name": "matrix-encrypt-attachment",
  "version": "0.3.0",
  "description": "Lbrary for encrypting and decrypting file attachments in Matrix.",
  "browser": "lib/browser-encrypt-attachment.js",
  "main": "lib/index.js",
  "scripts": {
    "prepublishOnly": "yarn build",
    "build": "yarn build:node && yarn build:browser",
    "build:node": "tsc",
    "build:browser": "browserify --standalone MatrixEncryptAttachment -d src/index.ts -p [ tsify -p ./tsconfig.json ] | exorcist lib/browser-encrypt-attachment.js.map > lib/browser-encrypt-attachment.js",
    "test": "yarn lint && yarn build && yarn test:browser",
    "test:browser": "karma start $KARMAFLAGS",
    "lint": "yarn lint:style",
    "lint:style": "eslint src test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/matrix-org/matrix-encrypt-attachment.git"
  },
  "keywords": [
    "matrix"
  ],
  "author": "matrix.org",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/matrix-org/matrix-encrypt-attachment/issues"
  },
  "homepage": "https://github.com/matrix-org/matrix-encrypt-attachment#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "browserify": "^17.0.0",
    "eslint": "^8.2.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-matrix-org": "github:matrix-org/eslint-plugin-matrix-org",
    "exorcist": "^2.0.0",
    "karma": "^6.3.8",
    "karma-chrome-launcher": "^3.1.0",
    "karma-firefox-launcher": "^2.1.2",
    "karma-mocha": "^2.0.1",
    "mocha": "^9.1.3",
    "tsify": "^5.0.4",
    "typescript": "^4.4.4"
  }
}
